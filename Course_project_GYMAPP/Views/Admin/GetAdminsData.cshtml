@model IEnumerable<Course_project_GYMAPP.Domain.Entity.Admin>

<table class="table table-hover">
<thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Ім'я</th>
    </tr>
</thead>
<tbody>
        @foreach (var user in Model)
        {
                    <tr>
                        <th scope="row">@user.Id</th>
                        <td>@user.Name</td>
                    </tr>
        }

</tbody>
</table>