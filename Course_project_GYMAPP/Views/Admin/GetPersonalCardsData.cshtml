@model IEnumerable<Course_project_GYMAPP.Domain.Entity.PersonalCard>

<table class="table table-hover">
<thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Ім'я</th>
        <th scope="col">Вартість</th>
        <th scope="col">Термін дії</th>
    </tr>
</thead>
<tbody>
        @foreach (var user in Model)
        {
            <tr>
                <th scope="row">@user.ID</th>
                <td>@user.Name</td>
                <td>@user.Price</td>
                <td>@user.Duration</td>
            </tr>
        }

</tbody>
</table>